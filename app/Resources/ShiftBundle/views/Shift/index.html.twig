{% extends "base.html.twig" %}

{% block title %}Staff your shifts on demand{% endblock %}
{% block head %}
    {{ parent() }}
{% endblock %}
{% block home_body %}
    <body data-spy="scroll" data-target="#navigation" data-offset="120">
    <!-- *** NAVBAR ***
    _________________________________________________________ -->

    <div class="navbar navbar-default navbar-fixed-top" role="navigation" id="navbar">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand scrollTo" href="#intro">Fill your shift</a>
            </div>

            <div class="navbar-collapse collapse" id="navigation">

                <ul class="nav navbar-nav navbar-right">
                    <li class="active"><a href="#intro">Intro</a>
                    </li>
                    <li><a href="#section1">What we do??</a>
                    </li>
                    <li><a href="#section3">Testimonials</a>
                    </li>
                    <li><a href="#section4">Contact</a>
                    </li>
                    {% if app.user.userType is defined %}
                        <li>
                            <a href="/dashboard" style="color: green; font-size: larger;"> {{ app.user.username }}</a>
                        </li>
                        <li>
                            <a href="{{ path('fos_user_security_logout') }}">
                                {{ 'layout.logout'|trans({}, 'FOSUserBundle') }}
                            </a>
                        </li>
                     {% else %}
                         <li>
                             <a href="#section2" style="color: green; font-size: larger;"> Login / Sign Up</a>
                         </li>
                    {% endif %}
                </ul>

            </div>
            <!--/.nav-collapse -->
        </div>
    </div>
    <!-- /#navbar -->

    <!-- *** NAVBAR END *** -->


    <div id="all">


        <!-- *** INTRO IMAGE ***
    _________________________________________________________ -->
        <div id="intro" class="clearfix">
            <div class="item">
                <div class="container">
                    <div class="row">

                        <h1 data-animate="fadeInDown">Make on demand work possible</h1>
                        <p class="message" data-animate="fadeInUp">
                            If you need to manage daily spikes in your workforce
                            we help you with quality people with a quick turn around Or
                            if you are in search of work for that extra bucks
                            which you need to save join us
                        </p>

                        <div class="col-md-6 col-md-offset-3" data-animate="fadeInUp">
                            <a class="btn btn-default" href="#section1">Read More</a>
                            {#<a class="btn btn-default" id="frm-landingPage1-submit" href="/user/register">Sign Up</a>#}
                            {#<a class="btn btn-default" id="frm-landingPage1-submit" href="/user/login">Login</a>#}
                        </div>

                    </div>
                </div>
            </div>
        </div>

        <!-- *** INTRO IMAGE END *** -->


        <!-- *** SERVICES ***
    _________________________________________________________ -->
        <div class="section" id="section1">
            <div class="container">
                <div class="col-md-12">
                    <h2 class="title" data-animate="fadeInDown">What we do?</h2>

                    <p class="lead">We help you manage work that needs to be done by providing employees who you can
                        depend upon when needed </p>

                    <div class="row services">

                        <div class="col-md-4" data-animate="fadeInUp">
                            <div class="icon"><i class="fa fa-home"></i>
                            </div>
                            <h3 class="heading">Great home</h3>
                            <p>Junk MTV quiz graced by fox whelps. </p>
                        </div>

                        <div class="col-md-4" data-animate="fadeInUp">
                            <div class="icon"><i class="fa fa-shopping-cart"></i>
                            </div>
                            <h3 class="heading">Stuff we would buy ourselves</h3>
                            <p>Bawds jog, flick quartz, vex nymphs. </p>
                        </div>

                        <div class="col-md-4" data-animate="fadeInUp">
                            <div class="icon"><i class="fa fa-euro"></i>
                            </div>
                            <h3 class="heading">Free trial</h3>
                            <p>Waltz, bad nymph, for quick jigs vex! Fox nymphs grab quick-jived waltz. </p>
                        </div>

                    </div>
                    <div class="row services">

                        <div class="col-md-4" data-animate="fadeInUp">
                            <div class="icon"><i class="fa fa-bullhorn"></i>
                            </div>
                            <h3 class="heading">We will propagate your stuff</h3>
                            <p>Brick quiz whangs jumpy veldt fox. Bright vixens jump; dozy fowl quack.</p>
                        </div>

                        <div class="col-md-4" data-animate="fadeInUp">
                            <div class="icon"><i class="fa fa-desktop"></i>
                            </div>
                            <h3 class="heading">Responsive web and app</h3>
                            <p>Quick wafting zephyrs vex bold Jim. Quick zephyrs blow, vexing daft Jim. </p>
                        </div>

                        <div class="col-md-4" data-animate="fadeInUp">
                            <div class="icon"><i class="fa fa-heart-o"></i>
                            </div>
                            <h3 class="heading">Dedicated support team</h3>
                            <p>Quick, Baz, get my woven flax jodhpurs! "Now fax quiz Jack!" my brave ghost pled. </p>
                        </div>

                    </div>

                </div>
                <!-- /.12 -->
            </div>
            <!-- /.container -->
        </div>
        <!-- /.section -->

        <!-- *** SERVICES END *** -->

        <!-- *** JOIN US ***
    _________________________________________________________ -->

        <div class="section" data-animate="bounceIn" id="section2">
            <div class="container">
                <div class="col-md-8 col-md-offset-2">
                    <h2 class="title"> Login / Sign Up</h2>
                    <p class="lead margin-bottom">
                        Login to our easy to use and flexible system and find the work you need and add the shifts you
                        want to fill in
                        if you are already a member please
                    </p>
                    <div class="row">
                        <div class="col-md-8 col-md-offset-2">
                            <span class="input-group-btn">
                                <a class="btn btn-default" id="frm-landingPage1-submit" href="/user/login">Login</a>
                            </span>
                            <span class="input-group-btn">
                                <a class="btn btn-default" id="frm-landingPage1-submit" href="/user/register">Sign Up</a>
                            </span>
                        </div>
                    </div>

                </div>
                <!-- /.12 -->
            </div>
            <!-- /.container -->
        </div>
        <!-- /.section -->

        <!-- *** JOIN US END *** -->

